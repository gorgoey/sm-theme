  {%- if show_sku -%}\n                                <span class=\"variant-item__sku break\">{{ sku | escape }}<\/span>\n                            {%- endif -%}\n                        <\/div>\n                        <div class=\"product-variant-sku\">\n                            <div class=\"variant-item__info\">\n                                <span class=\"variant-item__name h4 break\">{{ item.title | escape }}<\/span>\n                                {%- if show_sku -%}\n                                  <span class=\"variant-item__sku break\">{{ sku | escape }}<\/span>\n                                {%- endif -%}\n                              <\/div>\n                        <\/div>\n                    <\/div>\n                <\/li>\n            <\/ul>\n        <\/div>\n    <\/div>\n<\/section>",
